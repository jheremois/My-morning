{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@models/*": [
        "./src/models/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@router/*": [
        "./src/router/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@assets/*": [
        "./assets/*"
      ]
    }
  }
}